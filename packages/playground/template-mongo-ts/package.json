{
  "name": "express-mongodb-server-ts",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "A basic Express server with MongoDB (TypeScript version)",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec ts-node server.ts",
    "build": "tsc",
    "data:import": "ts-node utils/seeder.ts",
    "data:destroy": "ts-node utils/seeder.ts -d"
  },
  "keywords": [
    "express",
    "mongodb",
    "typescript",
    "api",
    "mvc"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
